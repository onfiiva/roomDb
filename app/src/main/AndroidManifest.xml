        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrynaRoomDb"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".AuthenticationActivity"/>
        <activity android:name=".AdminWindow"/>
        <activity android:name=".UserWindow"/>
        <activity android:name=".CardActivity"/>
        <activity android:name=".AccountActivity"/>
        <activity android:name=".AddCard"/>
        <activity android:name=".AddMoney"/>
        <activity android:name=".PaymentWindow"/>
        <activity android:name=".PaymentActivity"/>
        <activity android:name=".CardDiagram"/>
    </application>

</manifest>